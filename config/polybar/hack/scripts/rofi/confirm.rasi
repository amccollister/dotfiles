/* Confirm Dialog */

@import "colors.rasi"

* {
    background-color:       @bg;
    text-color:             @fg;
	font:				 	"Iosevka Nerd Font 16";
}

window {
    width:      			11%;
    padding:    			25px;
    border:					2px;
    border-radius:          0px;
    border-color:           @ac;
    location:               northeast;
    y-offset:               2.5%;
}

entry {
    expand: 				true;
    text-color:				@ac;
}
